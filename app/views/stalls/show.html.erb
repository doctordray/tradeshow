<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @stall.name %>
</p>

<p>
  <b>Stall number:</b>
  <%= @stall.stall_number %>
</p>

<p>
  <b>Logo:</b>
<% unless @stall.logo_file_name.nil? %>
  <%= image_tag(@stall.logo.url(:original)) %>
<% end %>
</p>

<p>
  <b>Address:</b>
  <%= @stall.address %>
</p>

<p>
  <b>Email:</b>
  <%= @stall.email %>
</p>
<p>
  <b>Url:</b>
  <%= @stall.url %>
</p>

<p>
  <b>Facebook:</b>
  <%= @stall.facebook %>
</p>

<p>
  <b>Twitter:</b>
  <%= @stall.twitter %>
</p>

<p>
  <b>Google+:</b>
  <%= @stall.google_plus %>
</p>

<p>
  <b>Description one:</b>
  <%= @stall.description_one %>
</p>

<p>
  <b>Description two:</b>
  <%= @stall.description_two %>
</p>


<%= link_to 'Edit', edit_stall_path(@stall) %> |
